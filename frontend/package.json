{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "backend:dev": "cd ../backend && ./mvnw spring-boot:run",
    "backend:build": "cd ../backend && ./mvnw clean package -DskipTests",
    "dev:all": "concurrently \"npm run dev\" \"npm run backend:dev\" --names \"FRONTEND,BACKEND\" --prefix-colors \"cyan,magenta\"",
    "build:all": "concurrently \"npm run build\" \"npm run backend:build\" --names \"FRONTEND,BACKEND\" --prefix-colors \"cyan,magenta\""
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "@tomtom-international/web-sdk-maps": "^6.25.0",
    "concurrently": "^9.1.2",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.540.0",
    "maplibre-gl": "^5.9.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "react-qr-code": "^2.0.18",
    "react-router-dom": "^7.8.1",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^9.1.2",
    "dompurify": "^3.2.6",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.3.0",
    "globals": "^16.3.0",
    "jsdom": "^27.0.0",
    "rc-slider": "^11.1.8",
    "vite": "^7.0.4",
    "vitest": "^3.2.4",
    "yet-another-react-lightbox": "^3.25.0"
  }
}
